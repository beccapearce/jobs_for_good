<%= form_for @job do |j| %>
  <%= j.label :title %>
  <%= j.text_field :title %>
  <%= j.label :company %>
  <%= j.text_field :company %>
  <%= j.label :location %>
  <%= j.text_field :location %>
  <%= j.label :description %>
  <%= j.text_field :description %>
  <%= j.label :salary %>
  <%= j.text_field :salary %>
  <%= j.submit %>
<% end %>
